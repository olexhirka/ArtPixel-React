    "react-dom": "^15.0.2",
    "react-hot-loader": "^1.3.0",
    "react-modal": "^1.2.1",
    "react-redux": "^4.0.0",
    "react-transform-hmr": "^1.0.0",
    "redux": "^3.0.4",
    "redux-undo": "^0.6.1",
    "style-loader": "^0.13.1",
