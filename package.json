    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "gm": "^1.21.1",
    "html-webpack-plugin": "^2.17.0",
    "immutable": "^3.7.5",
    "jquery": "^2.2.0",
    "js-cookie": "^2.0.4",
    "jsdom": "^8.5.0",
    "mocha": "^2.3.4",
    "node-localstorage": "^1.3.0",
    "oauth": "^0.9.14",
    "postcss-import": "^8.1.2",
